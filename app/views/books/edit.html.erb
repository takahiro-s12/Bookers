<h2>Editing Book</h2>
<%= form_with model:@book, url:"/books/#{@book.id}", local:true do |f| %>
  <h5>Title</h5>
  <%= f.text_field :title %>
  <h5>Body</h5>
  <%= f.text_area :body %>
  <p></p>
  <%= f.submit 'Update Book' %>
<% end %>
<%= link_to "Show", book_path(@book) %> | <%= link_to "Back", :back %>
